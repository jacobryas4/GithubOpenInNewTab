{
  "name": "GitHub Always Open in New Tab",
  "description": "Opens GitHub links in new tabs by default.",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "github_extension_logo_128.png",
    "48": "github_extension_logo_128.png",
    "128": "github_extension_logo_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*", "https://gist.github.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
